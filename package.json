{
  "name": "you-download-tube",
  "version": "0.1.0",
  "description": "a desktop application for downloading music from youtube",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaaseddik/YouDownloadTube.git"
  },
  "keywords": [
    "youtube",
    "music",
    "mp3"
  ],
  "author": "Mohamed Rafaa Seddik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaaseddik/YouDownloadTube/issues"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "dev": "nf start -p 3000",
    "build": "react-scripts build",
