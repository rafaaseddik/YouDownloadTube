    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^3.2.0",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "electron": "^4.0.1",
    "foreman": "^3.0.1"
